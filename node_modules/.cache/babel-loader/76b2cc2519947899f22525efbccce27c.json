ct v-model=\"value\" class=\"select\" placeholder=\"filter\">\r\n  <el-option\r\n    v-for=\"item in options\"\r\n    :key=\"item.value\"\r\n    :label=\"item.label\"\r\n    :value=\"item.value\"\r\n    \r\n  />\r\n</el-select>\r\n      <el-button type=\"warning\" round class=\"LRbutton\" @click=\"$router.push('/moviehub/dashboard/post/1234')\"\r\n          ><el-icon class=\"plus\"><Plus/></el-icon>Add Post</el-button>  \r\n      <el-divider />\r\n      <!--movie card-->\r\n      <el-space wrap>\r\n          <el-card v-for=\"(item,i) in movietitle\" :key=\"i\" class=\"box-card\" style=\"width: 250px;margin-right:20px\">\r\n            <template #header>\r\n              <div class=\"card-header\">\r\n                <span>{{movietitle[i].value}}</span>\r\n                <el-divider/>\r\n                <el-button class=\"button\" @click=\"jumptoContent(i)\">\r\n                    Detail review</el-button>\r\n                <span><el-image :src=\"movieposter[i].value\"></el-image></span>\r\n                <el-divider/>\r\n                <span>imdb rating:{{movieofficialrating[i].value}}</span>\r\n              </div>\r\n            </template>\r\n          </el-card>\r\n      </el-space>\r\n    </el-main>\r\n    \r\n  </el-container>\r\n  \r\n</div>\r\n<HubIcon/>\r\n</template>\r\n\r\n<script>\r\nimport { Search } from '@element-plus/icons-vue'\r\nimport { Plus } from '@element-plus/icons-vue'\r\nimport HubIcon from '@/components/HubIcon.vue';\r\nimport AvatarIcon from '@/components/AvatarIcon.vue';\r\nimport requestmovie from \"@/utils/Requestmovie.js\";\r\nimport SearchBar from '@/components/SearchBar.vue';\r\nimport request2 from \"@/utils/Request2.js\";\r\nexport default{\r\n    data(){\r\n        return{\r\n          components: {\r\n              AvatarIcon,\r\n              Plus,\r\n              Search,\r\n              HubIcon\r\n          },\r\n          input1:'',\r\n          value:'',\r\n          value2:'',\r\n          options : [\r\n          {\r\n            value: 'Drama',\r\n            label: 'Drama',\r\n          },\r\n          {\r\n            value: 'Comedy',\r\n            label: 'Comedy',\r\n          },\r\n          {\r\n            value: 'Horror',\r\n            label: 'Horror',\r\n          },\r\n          {\r\n            value: 'Thriller',\r\n            label: 'Thriller',\r\n          },\r\n          {\r\n            value: 'Action',\r\n            label: 'Action',\r\n          },\r\n          {\r\n            value: 'Sci-fi',\r\n            label: 'Sci-fi',\r\n          },\r\n          {\r\n            value: 'Crime',\r\n            label: 'Crime',\r\n          },\r\n          {\r\n            value: 'Adventure',\r\n            label: 'Adventure',\r\n          },\r\n          {\r\n            value: 'Other',\r\n            label: 'Other',\r\n          }\r\n          ],\r\n          optionsSort: [\r\n          {\r\n            value: 'A-Z Ascending',\r\n            label: 'alpha,asc',\r\n          },\r\n          {\r\n            value: 'A-Z Descending',\r\n            label: 'alpha,desc',\r\n          },\r\n          {\r\n            value: 'User Rating Ascending',\r\n            label: 'user_rating,asc',\r\n          },\r\n          {\r\n            value: 'User Rating Descending',\r\n            label: 'user_rating,desc',\r\n          },\r\n          {\r\n            value: 'Year Descending',\r\n            label: 'year,desc',\r\n          },\r\n          {\r\n            value: 'Year Ascending',\r\n            label: 'year,asc',\r\n          },\r\n          \r\n          ],\r\n          movietitle:[],\r\n          movieposter:[],\r\n          moviegenre:[],\r\n          movieofficialrating:[],\r\n          movieid:[],\r\n\r\n          itemlist: [],\r\n          //postlist:[],\r\n          movielist : [],\r\n          movieplot:[],\r\n          placeholder: \"search your movie\",\r\n          nodatatext: \"no input currently\",\r\n          selectValue: \"\",\r\n          last: \"\",\r\n