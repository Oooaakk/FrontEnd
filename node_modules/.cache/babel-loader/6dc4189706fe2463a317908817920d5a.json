 }\n      }); // 请求获取筛选列表\n    },\n\n    getTopmovies() {\n      request2.get(\"/?groups=top_250\").then(res => {\n        if (res.status === 200) {\n          for (let i = 0; i < res.data.items.length; i++) {\n            console.log(\"search movie\");\n            this.movietitle.push({\n              value: res.data.items[i].fullTitle,\n              label: res.data.items[i].fullTitle\n            });\n            this.movieofficialrating.push({\n              value: res.data.items[i].imDbRating,\n              label: res.data.items[i].imDbRating\n            });\n            this.movieposter.push({\n              value: res.data.items[i].image,\n              label: res.data.items[i].image\n            });\n            this.movieid.push({\n              value: res.data.items[i].id,\n              label: res.data.items[i].id\n            });\n          }\n        } else {\n          this.$message({\n            type: \"error\",\n            message: \"unsucessfully search\"\n          });\n        }\n      });\n    }\n\n  },\n  components: {\n    HubIcon,\n    Plus,\n    AvatarIcon,\n    SearchBar\n  }\n};","map":{"version":3,"mappings":";AAgEA,SAASA,MAAT,QAAuB,yBAAvB;AACA,SAASC,IAAT,QAAqB,yBAArB;AACA,OAAOC,OAAP,MAAoB,0BAApB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AACA,OAAOC,YAAP,MAAyB,yBAAzB;AACA,OAAOC,SAAP,MAAsB,4BAAtB;AACA,OAAOC,QAAP,MAAqB,qBAArB;AACA,eAAc;EACVC,IAAI,GAAE;IACF,OAAM;MACJC,UAAU,EAAE;QACRL,UADQ;QAERF,IAFQ;QAGRD,MAHQ;QAIRE;MAJQ,CADR;MAOJO,MAAM,EAAC,EAPH;MAQJC,KAAK,EAAC,EARF;MASJC,MAAM,EAAC,EATH;MAUJC,OAAM,EAAI,CACV;QACEF,KAAK,EAAE,OADT;QAEEG,KAAK,EAAE;MAFT,CADU,EAKV;QACEH,KAAK,EAAE,QADT;QAEEG,KAAK,EAAE;MAFT,CALU,EASV;QACEH,KAAK,EAAE,QADT;QAEEG,KAAK,EAAE;MAFT,CATU,EAaV;QACEH,KAAK,EAAE,UADT;QAEEG,KAAK,EAAE;MAFT,CAbU,EAiBV;QACEH,KAAK,EAAE,QADT;QAEEG,KAAK,EAAE;MAFT,CAjBU,EAqBV;QACEH,KAAK,EAAE,QADT;QAEEG,KAAK,EAAE;MAFT,CArBU,EAyBV;QACEH,KAAK,EAAE,OADT;QAEEG,KAAK,EAAE;MAFT,CAzBU,EA6BV;QACEH,KAAK,EAAE,WADT;QAEEG,KAAK,EAAE;MAFT,CA7BU,EAiCV;QACEH,KAAK,EAAE,OADT;QAEEG