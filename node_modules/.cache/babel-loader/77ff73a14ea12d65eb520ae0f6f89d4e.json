{"ast":null,"code":"import HubIcon from '@/components/HubIcon.vue';\nimport request from \"@/utils/RequestFile.js\";\nexport default {\n  data() {\n    return {\n      components: {\n        HubIcon\n      },\n      ruleForm: {\n        uname: \"\",\n        password: \"\"\n      },\n      rules: {\n        uname: [{\n          message: \"blank username！\",\n          trigger: \"blur\"\n        }],\n        password: [{\n          message: \"blank password！\",\n          trigger: \"blur\"\n        }]\n      },\n      loading: false\n    };\n  },\n\n  methods: {\n    submitForm(formName) {\n      console.log(\"submit\");\n      this.$refs[formName].validate(valid => {\n        // click login, login animation\n        this.loading = true;\n\n        if (valid) {\n          console.log('submit!');\n          request.post(\"/user/login\", {\n            email: this.ruleForm.uname,\n            password: this.ruleForm.password\n          }).then(res => {\n            if (res.status === 200) {\n              console.log(\"success\");\n              this.$router.push('/moviehub/mainpageuser/' + JSON.stringify(res.data));\n            }\n          });\n        } else {\n          console.log(\"error submit!!\");\n          this.loading = false;\n          return false;\n        }\n      });\n    },\n\n    resetForm(formName) {\n      this.$refs[formName].resetFields();\n    }\n\n  },\n  components: {\n    HubIcon\n  }\n};","map":{"version":3,"mappings":"AA+CA,OAAOA,OAAP,MAAoB,0BAApB;AACA,OAAOC,OAAP,MAAoB,wBAApB;AACA,eAAe;EACbC,IAAI,GAAG;IACH,OAAO;MACHC,UAAU,EAAE;QACRH;MADQ,CADT;MAIHI,QAAQ,EAAE;QACNC,KAAK,EAAE,EADD;QAENC,QAAQ,EAAE;MAFJ,CAJP;MAQHC,KAAK,EAAE;QACHF,KAAK,EAAE,CACH;UAAEG,OAAO,EAAE,iBAAX;UAA8BC,OAAO,EAAE;QAAvC,CADG,CADJ;QAIHH,QAAQ,EAAE,CACN;UAAEE,OAAO,EAAE,iBAAX;UAA8BC,OAAO,EAAE;QAAvC,CADM;MAJP,CARJ;MAgBHC,OAAO,EAAE;IAhBN,CAAP;EAkBH,CApBY;;EAqBbC,OAAO,EAAE;IACLC,UAAU,CAACC,QAAD,EAAW;MACjBC,OAAO,CAACC,GAAR,CAAY,QAAZ;MACJ,KAAKC,KAAL,CAAWH,QAAX,EAAqBI,QAArB,CAA+BC,KA