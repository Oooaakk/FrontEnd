    console.log(res)\r\n                    if (res.status===200) {\r\n                      this.getcontent=[];\r\n                      this.getrating=[];\r\n                      this.GetAllreviews();\r\n                      this.$message({\r\n                        //返回id movie card的\r\n                        type: \"success\",\r\n                        message: \"successfully post the movie\"\r\n                      })\r\n                    } else {\r\n                      this.$message({\r\n                        type: \"error\",\r\n                        message: \"fail to post due to unexpected reason\"\r\n                      })\r\n                    }\r\n                  })\r\n                  this.dialogVisible = false;\r\n          }else{\r\n            this.$message({\r\n                message: \"must complete post first!\",\r\n                type: \"error\",\r\n             })\r\n          }\r\n          })\r\n    },\r\n\r\n    \r\n  },\r\n  components:{HubIcon,AvatarIcon,Plus},\r\n\r\n  }\r\n  </script>\r\n  <style scoped>\r\n.demo-type {\r\n  display: flex;\r\n}\r\n.demo-type > div {\r\n  flex: 1;\r\n  text-align: center;\r\n}\r\n.text {\r\n  font-size: 14px;\r\n}\r\n.review_button{\r\n  position:fixed;\r\n   top:510px;\r\n   left:1110px;\r\n   color:orange;\r\n}\r\n.item {\r\n  padding: 18px 0;\r\n}\r\n\r\n/* .box-card {\r\n  width: 480px;\r\n} */\r\n.demo-type > div:not(:last-child) {\r\n  border-right: 1px solid var(--el-border-color);\r\n}\r\n.header{\r\n  background-color: black;\r\n\r\n}\r\n.select{\r\n  margin-left:10px;\r\n  width:10%;\r\n}\r\n.reviewcard{\r\n  position:relative;\r\n  top:-555px;\r\n  left:200px;\r\n\r\n}\r\n.recommend{\r\n  position:absolute;\r\n  font-size:x-large;\r\n  font-weight:bold;\r\n}\r\n.box-card{\r\nwidth: 30%;\r\nheight: 550px;\r\n\r\n}\r\n.textinput{\r\n   margin-bottom:20px;\r\n}\r\n.image{\r\n height:300px;\r\n margin-top: 10px;\r\n margin-bottom: 10px;\r\n}\r\n.imdb{\r\n  margin-bottom: 10px;\r