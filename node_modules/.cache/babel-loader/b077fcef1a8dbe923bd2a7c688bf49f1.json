{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport HubIcon from '@/components/HubIcon.vue';\nimport request from \"@/utils/RequestFile.js\";\nexport default {\n  data() {\n    return {\n      components: {\n        HubIcon\n      },\n      ruleForm: {\n        uname: \"\",\n        password: \"\"\n      },\n      rules: {\n        uname: [{\n          required: true,\n          message: \"email cannot be blank\",\n          trigger: \"blur\"\n        }, {\n          type: \"email\",\n          message: \"input correct email\",\n          trigger: \"change\"\n        }],\n        password: [{\n          required: true,\n          message: \"password cannot be blank\",\n          trigger: \"blur\"\n        }]\n      },\n      loading: false\n    };\n  },\n\n  methods: {\n    submitForm(formName) {\n      this.$refs[formName].validate(valid => {\n        // click login, login animation\n        this.loading = true; // if (valid) {\n        //   console.log(\"movie\")\n        //   request2.get(\"/k_gh3vr6fp\").then(res => {\n        //     console.log(\"success\");\n        //   })\n        // }\n\n        if (valid) {\n          console.log('submit!');\n          request.post(\"/user/login\", {\n            email: this.ruleForm.uname,\n            password: this.ruleForm.password\n          }).then(res => {\n            if (res.status === 200) {\n              console.log(\"success\");\n              window.localStorage.setItem(\"user\", JSON.stringify(res.data.access_token));\n              this.$router.push('/moviehub/mainpageuser/' + res.data.body.id);\n            }\n          }).catch(err => {\n            console.log(err);\n            this.$message({\n              message: \"incorrect password or email!\",\n              type: \"error\"\n            });\n            this.loading = false;\n            this.ruleForm.uname = '';\n            this.ruleForm.password = '';\n          });\n        } else {\n          console.log(\"error submit!!\");\n          this.loading = false;\n          return false;\n        }\n      });\n    },\n\n    resetForm(formName) {\n      this.$refs[formName].resetFields();\n    }\n\n  },\n  components: {\n    HubIcon\n  }\n};","map":{"version":3,"mappings":";AA+CA,OAAOA,OAAP,MAAoB,0BAApB;AACA,OAAOC,OAAP,MAAoB,wBAApB;AACA,eAAe;EACbC,IAAI,GAAG;IACH,OAAO;MACHC,UAAU,EAAE;QACRH;MADQ,CADT;MAIHI,QAAQ,EAAE;QACNC,KAAK,EAAE,EADD;QAENC,QAAQ,EAAE;MAFJ,CAJP;MAQHC,KAAK,EAAE;QACHF,KAAK,EAAE,CACD;UAAEG,QAAQ,EAAE,IAAZ;UAAkBC,OAAO,EAAE,uBAA3B;UAAoDC,OAAO,EAAE;QAA7D,CADC,EAED;UAAEC,IAAI,EAAE,OAAR;UAAiBF,OAAO,EAAE,qBAA1B;UAAiDC,OAAO,EAAE;QAA1D,CAFC,CADJ;QAKHJ,QAAQ,EAAE,CACJ;UAAEE,QAAQ,EAAE,IAAZ;UAAkBC,OAAO,EAAE,0BAA3B;UAAuDC,OAAO,EAAE;QAAhE,CADI;MALP,CARJ;MAiBHE,OAAO,EAAE;IAjBN,CAAP;EAmBH,CArBY;;EAsBbC,OAAO,EAAE;IACLC,UAAU,CAACC,QAAD,EAAW;MACjB,KAAKC,KAAL,CAAWD,QAAX,EAAqBE,QAArB,CAA+BC,KAAD,IAAW;QACzC;QACA,KAAKN,OAAL,GAAe,IAAf,CAFyC,CAGzC;QACA;QACA;QACA;QACA;QACA;;QACA,IAAIM,KAAJ,EAAW;UACPC,OAAO,CAACC,GAAR,CAAY,SAAZ;UACAnB,OAAO,CAACoB,IAAR,CAAa,aAAb,EAA4B;YAACC,KAAK,EAAE,KAAKlB,QAAL,CAAcC,KAAtB;YAA6BC,QAAQ,EAAE,KAAKF,QAAL,CAAcE;UAArD,CAA5B,EAA4FiB,IAA5F,CAAiGC,GAAE,IAAK;YAElG,IAAGA,GAAG,CAACC,MAAJ,KAAe,GAAlB,EAAsB;cACpBN,OAAO,CAACC,GAAR,CAAY,SAAZ;cACAM,MAAM,CAACC,YAAP,CAAoBC,OAApB,CAA4B,MAA5B,EAAoCC,IAAI,CAACC,SAAL,CAAeN,GAAG,CAACtB,IAAJ,CAAS6B,YAAxB,CAApC;cACA,KAAKC,OAAL,CAAaC,IAAb,CAAkB,4BAA0BT,GAAG,CAACtB,IAAJ,CAASgC,IAAT,CAAcC,EAA1D;YAEH;UACJ,CARD,EAQGC,KARH,CAQUC,GAAD,IAAO;YACdlB,OAAO,CAACC,GAAR,CAAYiB,GAAZ;YACA,KAAKC,QAAL,CAAc;cACR7B,OAAO,EAAE,8BADD;cAERE,IAAI,EAAE;YAFE,CAAd;YAIA,KAAKC,OAAL,GAAe,KAAf;YACA,KAAKR,QAAL,CAAcC,KAAd,GAAoB,EAApB;YACA,KAAKD,QAAL,CAAcE,QAAd,GAAuB,EAAvB;UACD,CAjBD;QAmBJ,CArBA,MAqBO;UACLa,OAAO,CAACC,GAAR,CAAY,gBAAZ;UAEA,KAAKR,OAAL,GAAe,KAAf;UACA,OAAO,KAAP;QACF;MACC,CApCD;IAqCH,CAvCI;;IAwCL2B,SAAS,CAACxB,QAAD,EAAW;MAChB,KAAKC,KAAL,CAAWD,QAAX,EAAqByB,WAArB;IACH;;EA1CI,CAtBI;EAkEbrC,UAAU,EAAE;IAAEH;EAAF;AAlEC,CAAf","names":["HubIcon","request","data","components","ruleForm","uname","password","rules","required","message","trigger","type","loading","methods","submitForm","formName","$refs","validate","valid","console","log","post","email","then","res","status","window","localStorage","setItem","JSON","stringify","access_token","$router","push","body","id","catch","err","$message","resetForm","resetFields"],"sourceRoot":"","sources":["D:\\comp30022 frontend\\comp30022-movie-review-website\\moviehub_frontend\\src\\views\\LoginPage.vue"],"sourcesContent":["<template>\r\n  <div id= 'building'>\r\n      <HubIcon></HubIcon>\r\n  <div >\r\n    <el-card class=\"box-card\">\r\n      <h2 class = \"color\">Login</h2>\r\n      <el-form\r\n        :model=\"ruleForm\"\r\n        status-icon\r\n        :rules=\"rules\"\r\n        ref=\"ruleForm\"\r\n        label-position=\"left\"\r\n        label-width=\"100px\"\r\n        class=\"login-from\"\r\n        hide-required-aterisk=\"false\"\r\n      >\r\n        <el-form-item class = \"color1\" label=\"Email\" prop=\"uname\" >\r\n          <el-input v-model=\"ruleForm.uname\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item class = \"color1\" label=\"Password\" prop=\"password\">\r\n          <el-input\r\n            type=\"password\"\r\n            v-model=\"ruleForm.password\"\r\n            autocomplete=\"off\"\r\n          ></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div>\r\n      <el-button @click=\"$router.push('/moviehub/forgetpassword')\" class=\"forgetpass\">Forgot password?</el-button>\r\n      </div>\r\n      <div class=\"btnGroup\">\r\n          \r\n        <el-button type=\"primary\" @click=\"submitForm('ruleForm')\"\r\n        v-loading=\"loading\"\r\n          >Login\r\n          </el-button>\r\n        <el-button @click=\"resetForm('ruleForm')\">Reset</el-button>\r\n        <router-link to=\"/moviehub/RegisterPage\">\r\n          <el-button style=\"margin-left:10px\">Register</el-button>\r\n        </router-link>\r\n      </div>\r\n    </el-c